{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/dev/assets/oh-my-opencode.schema.json",
  "auto_update": true,
  "background_task": {
    "defaultConcurrency": 3
  },
  "browser_automation_engine": {
    "provider": "agent-browser"
  },
  "new_task_system_enabled": true,
  "ralph_loop": {
    "enabled": true,
    "default_max_iterations": 100,
    "default_strategy": "continue"
  },
  "git_master": {
    "commit_footer": false,
    "include_co_authored_by": false
  },
  "tmux": {
    "enabled": true,
    "layout": "main-vertical",
    "main_pane_size": 60,
    "main_pane_min_width": 120,
    "agent_pane_min_width": 40
  },
  "experimental": {
    "task_system": true
  },
  "agents": {
    "sisyphus": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "oracle": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "librarian": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "explore": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "multimodal-looker": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "prometheus": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "metis": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "momus": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "atlas": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "ultrabrain": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "deep": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "artistry": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "quick": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "unspecified-low": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "unspecified-high": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    },
    "writing": {
      "model": "zai-coding-plan/glm-5",
      "temperature": 0
    }
  },
  "hashline_edit": true
}
