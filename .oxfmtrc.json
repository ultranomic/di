{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "objectWrap": "preserve",
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "ignorePatterns": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/node_modules",
    "**/.output",
    "**/.data",
    "**/.nuxt",
    "**/.nitro",
    "**/.cache",
    "**/dist",
    "**/.turbo",
    "**/pnpm-lock.yaml",
    "**/migrations",
    ".roomodes",
    ".roo/",
    "docs/",
    ".agents/skills"
  ],
  "embeddedLanguageFormatting": "auto",
  "insertFinalNewline": true
}
