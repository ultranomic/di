{
  "name": "voxel-monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "typecheck": "tsc --build --dry",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "prepublishOnly": "pnpm run typecheck && pnpm run test:coverage"
  },
  "devDependencies": {
    "typescript": "6.0.0-beta",
    "vitest": "4.0.18",
    "oxlint": "1.49.0",
    "oxfmt": "0.34.0",
    "@vitest/coverage-v8": "4.0.18"
  },
  "pnpm": {
    "overrides": {}
  }
}
